{
  "name": "soulprovidr.fm",
  "description": "Soul Provider FM website.",
  "version": "1.0.0",
  "author": "Shola Anozie <shola@soulprovidr.co>",
  "dependencies": {
    "bee-queue": "^1.2.2",
    "gatsby": "^2.0.0-rc.25",
    "gatsby-link": "^2.0.0-rc.3",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.1",
    "howler": "^2.0.15",
    "react-helmet": "^5.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "decrypt": "ansible-vault decrypt --vault-password-file .vault_pass",
    "develop": "gatsby develop",
    "encrypt": "ansible-vault encrypt --vault-password-file .vault_pass",
    "provision": "ansible-playbook site.yml -K --tags=nginx,redis,stream,storage --vault-password-file .vault_pass",
    "provision:app": "ansible-playbook site.yml -K --tags=app",
    "provision:nginx": "ansible-playbook site.yml -K --tags=nginx --vault-password-file .vault_pass",
    "provision:redis": "ansible-playbook site.yml -K --tags=redis --vault-password-file .vault_pass",
    "provision:storage": "ansible-playbook site.yml -K --tags=storage --vault-password-file .vault_pass",
    "provision:stream": "ansible-playbook site.yml -K --tags=stream --vault-password-file .vault_pass"
  },
  "devDependencies": {
    "gatsby-plugin-page-creator": "^1.0.1",
    "react": "^16.5.1",
    "react-dom": "^16.5.1"
  }
}
